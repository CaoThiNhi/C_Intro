#include<stdio.h>

int soNguyenTo(int so)
{
    int ktra = 0;
    int i;
    if (so < 2) ktra =1;
    else if(so ==2 ) ktra = 0;
    else
      for (i = 2; i <= sqrt(so); i ++)
        {
          if (so%i == 0)
            {
              ktra =1;
            }
        }
    return ktra;
}

