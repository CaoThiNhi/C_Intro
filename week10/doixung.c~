#include<stdio.h>

int doixung(char s[], int n){

  for( i=0; i<n; i++)
    if(s[i] != s[n-i])
      ktra = 0;
  return ktra;
}
int main(){
  char s[];
  int n, i;

  printf("So phan tu cua mang la: ");
  scanf("%d", &n);

  for( i=0; i<n; i++){
    printf("s[%d] = ", i+1);
    scanf("%d", &s[i]);
  }

  return 0;
}
